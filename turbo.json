{
  \"$schema\": \"https://turbo.build/schema.json\",
  \"globalDependencies\": [\"**/.env.*local\"],
  \"pipeline\": {
    \"build\": {
      \"dependsOn\": [\"^build\"],
      \"outputs\": [\".next/**\", \"!.next/cache/**\"]
    },
    \"lint\": {
      \"outputs\": []
    },
    \"typecheck\": {
      \"outputs\": []
    },
    \"test\": {
      \"outputs\": [\"coverage/**\"]
    },
    \"test:e2e\": {
      \"dependsOn\": [\"build\"],
      \"outputs\": [\"playwright-report/**\", \"test-results/**\"]
    },
    \"dev\": {
      \"cache\": false,
      \"persistent\": true
    }
  }
}"